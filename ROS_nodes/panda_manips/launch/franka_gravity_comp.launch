<?xml version="1.0" ?>
<launch>
  <arg name="robot_ip" />
  <arg name="load_gripper" default="true" />
  <include file="$(find panda_manips)/launch/franka_control_two.launch" >
  </include>

  <group ns="panda_table">
    <rosparam command="load" file="$(find franka_example_controllers)/config/franka_example_controllers.yaml" />
    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen"  args="force_example_controller"/>
   </group>
   <group ns="panda_box">
     <rosparam command="load" file="$(find franka_example_controllers)/config/franka_example_controllers.yaml" />
     <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen"  args="force_example_controller"/>
    </group>
    <node name="rqt_reconfigure" pkg="rqt_reconfigure" type="rqt_reconfigure" required="false" />
    <!--<node pkg="panda_capacity" name="panda_optimal_force" type="panda_optimal_force.py"/>-->
   </launch>
